<h1>仕入れ先の登録</h1>

<%= form_with model: @supplier, local: true do |f| %>
  <div>
    <%= f.label :company, '仕入れ先会社名' %>
    <%= f.text_field :company %>
  </div>
  <div>
    <%= f.label :city, '市区町村' %>
    <%= f.text_field :city %>
  </div>
  <div>
    <%= f.label :address, '住所' %>
    <%= f.text_field :address %>
  </div>
  <div>
    <%= f.label :phone, '電話番号' %>
    <%= f.text_field :phone %>
  </div>
  <div>
    <%= f.submit %>
  </div>
 <% end %>