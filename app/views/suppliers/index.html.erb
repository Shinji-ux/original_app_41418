<div class="table_container">
  <div class="section-title">
    仕入れ先の一覧
  </div>
  <table class="table-bordered">
    <thead>
      <tr>
        <th>会社名</th>
        <th>電話番号</th>
        <th>詳細</th>
      </tr>
    </thead>
    <tbody>
      <% @suppliers.each do |supplier| %>
        <tr>
          <td><%= supplier.company %></td>
          <td><%= supplier.phone %></td>
          <td><%= link_to '詳細', supplier_path(supplier.id) %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>