<h1><%= @supplier.company %>の詳細</h1>

<div>
  <label>会社名</label>
  <%= @supplier.company %>
</div>

<div>
  <label>市区町村</label>
  <%= @supplier.city %>
</div>

<div>
  <label>住所</label>
  <%= @supplier.address %>
</div>

<div>
  <label>電話番号</label>
  <%= @supplier.phone %>
</div>

<%= link_to '編集', edit_supplier_path(@supplier.id) %>
<%= link_to '削除', supplier_path(@supplier.id), data: { turbo_method: :delete, confirm: '削除してもよろしいですか？' } %>

<%= link_to '戻る', suppliers_path %>